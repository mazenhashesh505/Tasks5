#include <iostream>
#include <cmath>
using namespace std;


void
set_power (int arr[], int m, int len)
{
  //  cin>>m>>len;

  for (int i = 0; i < len; i++)
	{
	  arr[i] = pow (m, i);
	}
  for (int i = 0; i < len; i++)
	{
	  cout << arr[i] << "  ";
	}
}



int
main ()
{

  int arr[1000000], m, len;
  cin >> m >> len;

  set_power (arr, m, len);

  return 0;

}
