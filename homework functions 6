#include <iostream>
#include <cmath>
using namespace std;


bool
is_prime (int num)
{
  if (num <= 1)
	return false;

  for (int i = 2; i < num; i++)
	{
	  if (num % i == 0)
		return false;
	}
  return true;
}


int
main ()
{

  int num;
  for (int i = 0; i < (i + 1); i++)
	{
	  cin >> num;
	  cout << is_prime (num) << "\n";
	}

  return 0;

}
